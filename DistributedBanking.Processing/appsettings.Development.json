{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Kafka": {
    "Brokers": "localhost:9097",
    "Connections": {
      "Account": "account-topic-name",
      "Transaction": "transaction-topic-name"
    },
    "Consumers": {
      "Account": {
        "AutoOffsetReset": "Latest",
        "EnableAutoCommit": true,
        "SessionTimeoutMs": 30000,
        "GroupId": "processing-group"
      },
      "Transaction": {
        "AutoOffsetReset": "Latest",
        "EnableAutoCommit": true,
        "SessionTimeoutMs": 30000,
        "GroupId": "processing-group"
      }
    }
  }
}
